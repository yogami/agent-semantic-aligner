{
  "name": "agent-semantic-aligner",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --coverage.thresholds.lines=80 --coverage.thresholds.branches=80",
    "test:e2e": "playwright test",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint:duplication": "jscpd src --threshold 5",
    "quality": "npm run lint && npm run lint:duplication && npm run test:coverage"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "dotenv": "^17.2.3",
    "next": "^16.1.1",
    "openai": "^6.10.0",
    "prisma": "^7.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.0.0-beta.8",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "jscpd": "^4.0.5",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.0.0-beta.8",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
